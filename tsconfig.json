{
  "compilerOptions": {
    "outDir": "./build/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6"
  },
  "include": [
    //        "./src/**/*",
    "./backend/**/*"
  ]
}